  "homepage": "https://brain.fm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "dependencies": {
    "electron": "1.7.9",
    "electron-packager": "10.1.0"
  }
}
